workshop event OngoingEachPlayer(team: Team, heroSlot: Hero | Slot) {
    val player: Player
}

workshop event OngoingGlobal(team: Team, heroSlot: Hero | Slot) { }

workshop open struct Player {
	workshop getval hero: Hero

	workshop setvar ability1Enabled: bool
	workshop setvar isFiringSecondary: bool
	workshop setvar damageDealt: num

    workshop fn heal(amount: num, healer: Player = null)

    workshop fn applyImpulse(direction: Vector, speed: num, relative: Relative, motion: Motion)

    workshop fn raycast(position: Location, direction: Vector, maxDistance: num): bool
}

workshop struct Vector {
	workshop var x: num
	workshop var y: num
	workshop var z: num

	workshop static const ZERO: Vector
}


workshop event OngoingEachPlayer(team: Team, heroSlot: Hero | Slot) {
    self(player <- workshop.player)
}

workshop event OngoingGlobal() {
    self()
}

workshop event PlayerDealtDamage(team: Team, heroSlot: Hero | Slot) {
    self(
        player <- workshop.player,
        attacker <- workshop.attacker,
        victim <- workshop.victim
    )
}

workshop struct Player {
	workshop var ability1Enabled: Bool
	workshop val hero: Hero
	workshop var isFiringSecondary: Bool

    workshop func heal(amount: number, healer: Player = null)

    workshop func applyImpulse(direction: Vector, speed: Number, relative: Relative, motion: Motion)
}

workshop func raycast(position: Locatable, direction: Vector, maxDistance: Number): Bool

workshop struct Vector {
	workshop var x: number
	workshop var y: number
	workshop var z: number

	workshop static func zero(): Vector
}

workshop enum Relative {
	ToWorld,
	ToPlayer
}


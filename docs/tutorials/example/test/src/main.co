import std;

rule "Hello World" OngoingEachPlayer() {
    cond (player.a && player.b) || player.c;

    player.counter += 1;
    player.counter -= 1;
    player.counter *= 1;
    player.counter /= 1;
    player.counter = 10;
    player.damageDealt = 150;
    player.sendSmallMessage("Hello World");
}